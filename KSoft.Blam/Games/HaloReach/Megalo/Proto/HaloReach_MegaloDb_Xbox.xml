		<MaxUserDefinedOptionValues>8</MaxUserDefinedOptionValues>
		<MaxVariantStrings>112</MaxVariantStrings>
		<MaxPlayerTraits>16</MaxPlayerTraits>
		<MaxGameStatistics>4</MaxGameStatistics>
		<MaxHudWidgets>4</MaxHudWidgets>
