		<StreamConditionsAndActionsRefsAsRefs>true</StreamConditionsAndActionsRefsAsRefs>
		<MaxConditions>512</MaxConditions>
		<MaxActions>1024</MaxActions>
		<MaxTriggers>320</MaxTriggers>

